{
    "private": true,
    "license": "MIT",
    "name": "template_typescript",
    "version": "0.0.0",
    "description": "A boilerplate project for typescript",
    "author": "Marcelo Pereira <marcgp@gmail.com>",
    "repository": "https://github.com/marcgp/template_typescript.git",
    "bugs": "https://github.com/marcgp/template_typescript/issues",
    "homepage": "https://github.com/marcgp/template_typescript#readme",
    "scripts": {
        "dev": "nodemon --watch source --exec ts-node source/main.ts",
        "lint": "eslint . --ext .ts",
        "build": "npm run lint && npm run build:clean && npm run build:bundles",
        "build:clean": "rm -rf ./build",
        "build:bundles": "npm run build:bundle:main",
        "build:bundle:main": "webpack --config ./webpack.config.js"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "7.8.4",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "7.8.3",
        "@babel/preset-typescript": "7.8.3",
        "@types/fs-extra": "8.1.0",
        "@types/gulp": "4.0.6",
        "@types/node": "13.7.4",
        "@types/webpack-node-externals": "1.7.1",
        "@typescript-eslint/parser": "2.20.0",
        "babel-loader": "8.0.6",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-plugin-prettier": "3.1.2",
        "fs-extra": "8.1.0",
        "gulp": "4.0.2",
        "gulp-cli": "2.2.0",
        "nodemon": "2.0.2",
        "prettier": "1.19.1",
        "ts-node": "8.6.2",
        "typescript": "3.7.5",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.11",
        "webpack-node-externals": "1.7.2",
        "webpack-stream": "5.2.1"
    }
}
